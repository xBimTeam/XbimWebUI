{
   "opts":{
      "tutorials":"Tutorials",
      "destination":"..\\..\\XbimWebUI.Docs",
      "template":"Resources/doctemplate"
   },
   "tags":{
      "allowUnknownTags":true
   },
   "source":{
      "include":[
        "Viewer\\xbim-viewer.debug.js",
        "Viewer\\xbim-state.debug.js",
        "Viewer\\xbim-product-type.debug.js",
		"Browser\\xbim-browser.debug.js",
		"Browser\\xbim-visual-assignment-set.debug.js",
		"Browser\\xbim-visual-attribute.debug.js",
		"Browser\\xbim-visual-entity.debug.js",
		"Browser\\xbim-visual-model.debug.js",
		"Browser\\xbim-visual-property.debug.js",
		"Plugins\\NavigationCube\\xbim-navigation-cube.debug.js",
        "Resources/README.md"
      ],
      "includePattern":".+\\.js(doc)?$",
      "excludePattern":"(^|\\/|\\\\)_"
   },
   "plugins":[
      "plugins/markdown"
   ],
   "templates":{
      "cleverLinks":false,
      "monospaceLinks":false,
	  "default": {
            "outputSourceFiles": false
        }
   }
}